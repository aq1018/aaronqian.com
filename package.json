  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@eslint/css": "^0.13.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vitest/ui": "^4.0.1",
    "eslint": "^9.38.0",
    "eslint-config-love": "^133.0.0",
